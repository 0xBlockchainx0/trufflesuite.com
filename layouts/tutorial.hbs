<!DOCTYPE html>
<html>
<head>
  {{> ga-tag-head}}
  {{> hs-tag}}
  
  <meta charset="utf-8">
  <meta name="description" content="{{#if social-description}}{{ social-description }}{{else}}The Truffle suite of tools make dapp development easier and more consistent.{{/if}}">
  {{#if keywords}}<meta name="keywords" content="{{ keywords }}">{{/if}}
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  
  <meta name="environment" content="{{ NODE_ENV }}">

  <!-- Favicon -->
  <link rel="icon" href="/img/favicons/favicon.ico">

  <!-- Add to homescreen for Chrome on Android -->
  <meta name="mobile-web-app-capable" content="yes">
  <meta name="application-name" content="Truffle Suite">
  <link rel="icon" sizes="192x192" href="/img/favicons/chrome-touch-icon-192x192.png">

  <!-- Add to homescreen for Safari on iOS -->
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black">
  <meta name="apple-mobile-web-app-title" content="Truffle Suite">
  <link rel="apple-touch-icon" href="/img/favicons/apple-touch-icon.png">

  <!-- Tile icon for Win8/10 (144x144 + tile color) -->
  <meta name="msapplication-TileImage" content="img/favicons/ms-touch-icon-144x144-precomposed.png">
  <meta name="msapplication-TileColor" content="#3372DF">

  <!-- Facebook OpenGraph -->
  <meta property="og:site_name" content="Truffle Suite" />
  <meta property="og:description" content="{{#if social-description}}{{ social-description }}{{else}}The Truffle suite of tools make dapp development easier and more consistent.{{/if}}" />
  <meta property="og:title" content="{{#if parent-title}}{{ title }} | {{ parent-title }}{{else}}{{ title }}{{/if}} | Truffle Suite" />
  <meta property="og:url" content="{{getPageUrl path}}" />
  <meta property="og:type" content="article" />
  <meta property="og:image" content="{{#if social-image}}{{ social-image }}{{else}}https://truffleframework.com/img/favicons/truffle-share.png{{/if}}" />

  <!-- Twitter Card -->
  <meta name="twitter:card" content="summary" />
  <meta name="twitter:site" content="Truffle Suite" />
  <meta name="twitter:title" content="{{#if parent-title}}{{ title }} | {{ parent-title }}{{else}}{{ title }}{{/if}} | Truffle Suite" />
  <meta name="twitter:description" content="{{#if social-description}}{{ social-description }}{{else}}The Truffle suite of tools make dapp development easier and more consistent.{{/if}}" />
  <meta name="twitter:creator" content="Truffle Suite" />
  <meta name="twitter:domain" content="trufflesuite.com" />
  <meta name="twitter:image:src" content="{{#if social-image}}{{ social-image }}{{else}}https://truffleframework.com/img/favicons/truffle-share.png{{/if}}" />
  
  <meta name="google-site-verification" content="BSgPkMHzw7IxJTpEElNfD8ZZYPzXgOQiTVPzAxAG8-o" />

  {{#if parent-title}}
    <title>{{ title }} | {{ parent-title }} | Truffle Suite</title>
  {{else}}
    <title>{{ title }} | Truffle Suite</title>
  {{/if}}

  <link href="https://fonts.googleapis.com/css?family=Grand+Hotel|Open+Sans|Oswald|Varela+Round|Roboto+Condensed|Roboto+Mono|Cinzel" rel="stylesheet">
  <link rel="stylesheet" href="https://use.typekit.net/okj1vue.css">
  <script src="https://kit.fontawesome.com/371f2d7a8a.js" crossorigin="anonymous"></script>
  <link rel="stylesheet" href="/css/index.css?cache_buster={{cachebuster}}">

  {{> mixpanel-tag-head}}
</head>
<body{{#if light-body}} class="light"{{/if}}>
  {{> ga-tag-body}}
  
  {{> navbar}}

  {{!-- <main class="container container-heading container-narrow">
    <div class="row">
      <div class="col">
        <h1>{{ title }}</h1>
        <ul class="list-sm-stacker space-icon mb-0">
          <li><i class="far fa-calendar-alt"></i> {{getMetadata "tutorials" path.name "date"}}&nbsp;{{#if (metadataExists "tutorials" path.name "updated")}}(updated {{getMetadata "tutorials" path.name "updated"}}){{/if}}&nbsp;&nbsp;</li>
          <li><i class="far fa-user"></i> {{getMetadata "tutorials" path.name "author"}}</li>
        </ul>
      </div>
    </div>
  </main> --}}

  <main class="banner banner-light-cream pt-5 pb-5" role="main">
    <div class="container container-post">
      <div class="row justify-content-center">
        <div class="col">
          {{{ contents }}}

          {{> contrib-link}}
        </div>
      </div>
    </div>
  </main>

  {{> footer}}

  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
  <script>window.jQuery || document.write('<script src="/js/vendor/jquery-3.2.1.slim.min.js"><\/script>')</script>
  <script src="/js/bootstrap.min.js"></script>
  <script src="/js/vendor/prism.js"></script>
  <script src="/js/markdown-cleanup.js"></script>
</body>
</html>